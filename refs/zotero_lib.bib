	pages = {162--174},
	file = {Gonzalez-Mirelis et al. - 2014 - Using Vessel Monitoring System Data to Improve Sys.pdf:/Users/caseyohara/Library/Application Support/Zotero/Profiles/hvirdo1j.default/zotero/storage/625VIMXB/Gonzalez-Mirelis et al. - 2014 - Using Vessel Monitoring System Data to Improve Sys.pdf:application/pdf;Snapshot:/Users/caseyohara/Library/Application Support/Zotero/Profiles/hvirdo1j.default/zotero/storage/TVITKR76/s13280-013-0413-7.html:text/html}
