@article{jenkins_expansion_2009,
	title = {Expansion of the global terrestrial protected area system},
	volume = {142},
	issn = {0006-3207},
	url = {http://www.sciencedirect.com/science/article/pii/S0006320709001980},
	doi = {10.1016/j.biocon.2009.04.016},
	abstract = {Under the Convention on Biological Diversity, the world’s governments set a goal of protecting 10\% of all ecological regions by 2010. We evaluated progress toward that goal for the world’s major terrestrial biomes, realms, and ecoregions. Total land area under any legal protection has increased from previous estimates to 12.9\%, a notable achievement, although only 5.8\% has strict protection for biodiversity. For biomes, protection ranges from 4\% to 25\%, with six of 14 biomes still below the 10\% level. Geographic patterns of protection have a distinct bias, with higher rates of protection in New World realms than Old World realms. Of the world’s terrestrial ecoregions, half do not meet the 2010 Target and 76\% have less than 10\% of their area strictly protected. Approximately 13\% of ecoregions have no strict protected areas. Recent years have seen an expansion of the protected area network, with an average of 0.13\% of the global land area added per year. Most of the expansion since 2003 though has been in Brazil, particularly the Amazon. Without major investments in conservation, spread across the world’s ecosystems, the world will likely miss the 2010 target.},
	number = {10},
	urldate = {2014-04-18},
	journal = {Biological Conservation},
	author = {Jenkins, Clinton N. and Joppa, Lucas},
	month = oct,
	year = {2009},
	keywords = {Biodiversity, Biome, Conservation planning, ecoregion, Prioritization, Realm},
	pages = {2166--2174},
	file = {ScienceDirect Full Text PDF:/Users/caseyohara/Library/Application Support/Zotero/Profiles/hvirdo1j.default/zotero/storage/AXFDQWGE/Jenkins and Joppa - 2009 - Expansion of the global terrestrial protected area.pdf:application/pdf;ScienceDirect Snapshot:/Users/caseyohara/Library/Application Support/Zotero/Profiles/hvirdo1j.default/zotero/storage/Z4IBN9VG/S0006320709001980.html:text/html}
}

@article{hampton_tao_2015,
	title = {The {Tao} of open science for ecology},
	volume = {6},
	issn = {2150-8925},
	url = {http://onlinelibrary.wiley.com/doi/10.1890/ES14-00402.1/abstract},
	doi = {10.1890/ES14-00402.1},
	abstract = {The field of ecology is poised to take advantage of emerging technologies that facilitate the gathering, analyzing, and sharing of data, methods, and results. The concept of transparency at all stages of the research process, coupled with free and open access to data, code, and papers, constitutes “open science.” Despite the many benefits of an open approach to science, a number of barriers to entry exist that may prevent researchers from embracing openness in their own work. Here we describe several key shifts in mindset that underpin the transition to more open science. These shifts in mindset include thinking about data stewardship rather than data ownership, embracing transparency throughout the data life-cycle and project duration, and accepting critique in public. Though foreign and perhaps frightening at first, these changes in thinking stand to benefit the field of ecology by fostering collegiality and broadening access to data and findings. We present an overview of tools and best practices that can enable these shifts in mindset at each stage of the research process, including tools to support data management planning and reproducible analyses, strategies for soliciting constructive feedback throughout the research process, and methods of broadening access to final research products.},
	language = {en},
	number = {7},
	urldate = {2016-08-01},
	journal = {Ecosphere},
	author = {Hampton, Stephanie E. and Anderson, Sean S. and Bagby, Sarah C. and Gries, Corinna and Han, Xueying and Hart, Edmund M. and Jones, Matthew B. and Lenhardt, W. Christopher and MacDonald, Andrew and Michener, William K. and Mudge, Joe and Pourmokhtarian, Afshin and Schildhauer, Mark P. and Woo, Kara H. and Zimmerman, Naupaka},
	month = jul,
	year = {2015},
	keywords = {data management, Ecology, open access, Open science, Reproducible research},
	pages = {1--13},
	file = {Full Text PDF:/Users/caseyohara/Library/Application Support/Zotero/Profiles/hvirdo1j.default/zotero/storage/MGEKI7GS/Hampton et al. - 2015 - The Tao of open science for ecology.pdf:application/pdf;Snapshot:/Users/caseyohara/Library/Application Support/Zotero/Profiles/hvirdo1j.default/zotero/storage/V2TDTKJS/Hampton et al. - 2015 - The Tao of open science for ecology.html:text/html}
